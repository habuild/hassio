debugMQTT="$Debug_MQTT"    #  debug   '-d' or '--quiet'
RoK='-r'               #  retain  '-r' or  '-d' depending on value above 

#  Array stuff     ####    https://stackoverflow.com/questions/3685970/check-if-a-bash-array-contains-a-value
#if [[ " ${array[*]} " =~ " ${value} " ]]; then                     
